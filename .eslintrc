{
	"extends": ["plugin:flowtype/recommended", "airbnb", "prettier", "prettier/react"],
	"plugins": ["flowtype", "prettier"],
	"parser": "babel-eslint",
	"rules": {
		"linebreak-style": 0
	},
	"parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	}
}
